SQL: UPDATE `article_data` SET `des`='Zabbix',`keywords`='Zabbix',`content`='&lt;p style=&quot;padding: 0px; color: rgb(51, 51, 51); font-family: &amp;quot;Microsoft Yahei&amp;quot;, Helvetica, Arial, sans-serif; font-size: 14px; margin-top: 0px; margin-bottom: 10px; white-space: normal; box-sizing: border-box; background-color: rgb(255, 255, 255);&quot;&gt;&lt;strong style=&quot;margin: 0px; padding: 0px; box-sizing: border-box;&quot;&gt;一、nginx开启stub_status模块&lt;/strong&gt;&lt;br style=&quot;margin: 0px; padding: 0px; box-sizing: border-box;&quot;/&gt;&lt;/p&gt;&lt;p style=&quot;padding: 0px; color: rgb(51, 51, 51); font-family: &amp;quot;Microsoft Yahei&amp;quot;, Helvetica, Arial, sans-serif; font-size: 14px; margin-top: 0px; margin-bottom: 10px; white-space: normal; box-sizing: border-box; background-color: rgb(255, 255, 255);&quot;&gt;&lt;strong style=&quot;margin: 0px; padding: 0px; box-sizing: border-box;&quot;&gt;&amp;nbsp; 1、添加stub_status模块&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;padding: 0px; color: rgb(51, 51, 51); font-family: &amp;quot;Microsoft Yahei&amp;quot;, Helvetica, Arial, sans-serif; font-size: 14px; margin-top: 0px; margin-bottom: 10px; white-space: normal; box-sizing: border-box; background-color: rgb(255, 255, 255);&quot;&gt;&lt;strong style=&quot;margin: 0px; padding: 0px; box-sizing: border-box;&quot;&gt;&amp;nbsp;&amp;nbsp;&lt;/strong&gt;在编译安装nginx的时候添加“&lt;span style=&quot;margin: 0px; padding: 0px; font-family: &amp;quot;Times New Roman&amp;quot;, serif; box-sizing: border-box;&quot;&gt;--with-http_stub_status_module&lt;/span&gt;”模块。&lt;/p&gt;&lt;pre class=&quot;brush:bash;toolbar:false&quot; style=&quot;padding: 9.5px; border-radius: 4px; border: 1px solid rgb(204, 204, 204); border-image: none; color: rgb(51, 51, 51); line-height: 1.4285; overflow: auto; font-family: Menlo, Monaco, Consolas, &amp;quot;Courier New&amp;quot;, monospace; font-size: 13px; margin-top: 0px; margin-bottom: 10px; -ms-word-break: break-all; -ms-word-wrap: break-word; box-sizing: border-box; background-color: rgb(245, 245, 245);&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;./configure&amp;nbsp;--prefix=/usr/local/nginx&amp;nbsp;--with-http_stub_status_module&lt;/pre&gt;&lt;p style=&quot;padding: 0px; color: rgb(51, 51, 51); font-family: &amp;quot;Microsoft Yahei&amp;quot;, Helvetica, Arial, sans-serif; font-size: 14px; margin-top: 0px; margin-bottom: 10px; white-space: normal; box-sizing: border-box; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;&lt;span style=&quot;margin: 0px; padding: 0px; font-family: &amp;quot;Times New Roman&amp;quot;, serif; font-size: 16px; box-sizing: border-box;&quot;&gt;&lt;/span&gt;&lt;br style=&quot;margin: 0px; padding: 0px; box-sizing: border-box;&quot;/&gt;&lt;/p&gt;&lt;p style=&quot;padding: 0px; color: rgb(51, 51, 51); font-family: &amp;quot;Microsoft Yahei&amp;quot;, Helvetica, Arial, sans-serif; font-size: 14px; margin-top: 0px; margin-bottom: 10px; white-space: normal; box-sizing: border-box; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;&amp;nbsp;&lt;strong style=&quot;margin: 0px; padding: 0px; box-sizing: border-box;&quot;&gt;2、nginx配置文件中开启status&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;padding: 0px; color: rgb(51, 51, 51); font-family: &amp;quot;Microsoft Yahei&amp;quot;, Helvetica, Arial, sans-serif; font-size: 14px; margin-top: 0px; margin-bottom: 10px; white-space: normal; box-sizing: border-box; background-color: rgb(255, 255, 255);&quot;&gt;&lt;strong style=&quot;margin: 0px; padding: 0px; box-sizing: border-box;&quot;&gt;&amp;nbsp;&amp;nbsp;&lt;/strong&gt;在站点server下添加一个location&lt;strong style=&quot;margin: 0px; padding: 0px; box-sizing: border-box;&quot;&gt;。&lt;/strong&gt;&lt;/p&gt;&lt;pre class=&quot;brush:bash;toolbar:false&quot; style=&quot;padding: 9.5px; border-radius: 4px; border: 1px solid rgb(204, 204, 204); border-image: none; color: rgb(51, 51, 51); line-height: 1.4285; overflow: auto; font-family: Menlo, Monaco, Consolas, &amp;quot;Courier New&amp;quot;, monospace; font-size: 13px; margin-top: 0px; margin-bottom: 10px; -ms-word-break: break-all; -ms-word-wrap: break-word; box-sizing: border-box; background-color: rgb(245, 245, 245);&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;server&amp;nbsp;{
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;listen&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;80;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;server_name&amp;nbsp;&amp;nbsp;www.xxx.com;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;location&amp;nbsp;/&amp;nbsp;{
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;root&amp;nbsp;html;
&amp;nbsp;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;}
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;location&amp;nbsp;/ngxstatus&amp;nbsp;{
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;stub_status;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;access_log&amp;nbsp;off;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;allow&amp;nbsp;127.0.0.1
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;allow&amp;nbsp;192.168.0.0/24;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;deny&amp;nbsp;all;
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;}
&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;}&lt;/pre&gt;&lt;p style=&quot;padding: 0px; color: rgb(51, 51, 51); font-family: &amp;quot;Microsoft Yahei&amp;quot;, Helvetica, Arial, sans-serif; font-size: 14px; margin-top: 0px; margin-bottom: 10px; white-space: normal; box-sizing: border-box; background-color: rgb(255, 255, 255);&quot;&gt;&lt;br style=&quot;margin: 0px; padding: 0px; box-sizing: border-box;&quot;/&gt;&lt;/p&gt;&lt;p style=&quot;padding: 0px; color: rgb(51, 51, 51); font-family: &amp;quot;Microsoft Yahei&amp;quot;, Helvetica, Arial, sans-serif; font-size: 14px; margin-top: 0px; margin-bottom: 10px; white-space: normal; box-sizing: border-box; background-color: rgb(255, 255, 255);&quot;&gt;&lt;strong style=&quot;margin: 0px; padding: 0px; box-sizing: border-box;&quot;&gt;二、编写脚本获取nginx status&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;padding: 0px; color: rgb(51, 51, 51); font-family: &amp;quot;Microsoft Yahei&amp;quot;, Helvetica, Arial, sans-serif; font-size: 14px; margin-top: 0px; margin-bottom: 10px; white-space: normal; box-sizing: border-box; background-color: rgb(255, 255, 255);&quot;&gt;&lt;strong style=&quot;margin: 0px; padding: 0px; box-sizing: border-box;&quot;&gt;&amp;nbsp; 1、脚本&lt;/strong&gt;&lt;/p&gt;&lt;pre class=&quot;brush:bash;toolbar:false&quot; style=&quot;padding: 9.5px; border-radius: 4px; border: 1px solid rgb(204, 204, 204); border-image: none; color: rgb(51, 51, 51); line-height: 1.4285; overflow: auto; font-family: Menlo, Monaco, Consolas, &amp;quot;Courier New&amp;quot;, monospace; font-size: 13px; margin-top: 0px; margin-bottom: 10px; -ms-word-break: break-all; -ms-word-wrap: break-word; box-sizing: border-box; background-color: rgb(245, 245, 245);&quot;&gt;&amp;nbsp;#!/bin/bash
&amp;nbsp;&amp;nbsp;nginx_active(){
&amp;nbsp;&amp;nbsp;/usr/bin/curl&amp;nbsp;&amp;quot;http://127.0.0.1/ngxstatus&amp;quot;&amp;nbsp;2&amp;gt;/dev/null|&amp;nbsp;grep&amp;nbsp;&amp;#39;Active&amp;#39;&amp;nbsp;|&amp;nbsp;awk&amp;nbsp;&amp;#39;{print&amp;nbsp;$NF}&amp;#39;
&amp;nbsp;}
&amp;nbsp;&amp;nbsp;nginx_reading(){
&amp;nbsp;&amp;nbsp;/usr/bin/curl&amp;nbsp;&amp;quot;http://127.0.0.1/ngxstatus&amp;quot;&amp;nbsp;2&amp;gt;/dev/null|&amp;nbsp;grep&amp;nbsp;&amp;#39;Reading&amp;#39;&amp;nbsp;|&amp;nbsp;awk&amp;nbsp;&amp;#39;{print&amp;nbsp;$2}&amp;#39;
&amp;nbsp;}
&amp;nbsp;&amp;nbsp;nginx_writing(){
&amp;nbsp;&amp;nbsp;/usr/bin/curl&amp;nbsp;&amp;quot;http://127.0.0.1/ngxstatus&amp;quot;&amp;nbsp;2&amp;gt;/dev/null|&amp;nbsp;grep&amp;nbsp;&amp;#39;Writing&amp;#39;&amp;nbsp;|&amp;nbsp;awk&amp;nbsp;&amp;#39;{print&amp;nbsp;$4}&amp;#39;
&amp;nbsp;}
&amp;nbsp;&amp;nbsp;nginx_waiting(){
&amp;nbsp;&amp;nbsp;/usr/bin/curl&amp;nbsp;&amp;quot;http://127.0.0.1/ngxstatus&amp;quot;&amp;nbsp;2&amp;gt;/dev/null|&amp;nbsp;grep&amp;nbsp;&amp;#39;Waiting&amp;#39;&amp;nbsp;|&amp;nbsp;awk&amp;nbsp;&amp;#39;{print&amp;nbsp;$6}&amp;#39;
&amp;nbsp;}
