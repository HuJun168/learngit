SQL: INSERT INTO `article_data` (`des`,`keywords`,`content`,`article_aid`) VALUES ('Windows server','Windows server','&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;一、事因：Windows Server 2012莫名宕机&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp; 查看系统日志：&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp; &amp;nbsp;&lt;img src=&quot;/Upload/ueditor/image/20170109/1483942662878450.png&quot; title=&quot;1478504106157441.png&quot; alt=&quot;20161107142903.png&quot; width=&quot;630&quot; height=&quot;126&quot; style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; border: none; vertical-align: middle; width: 630px; height: 126px;&quot;/&gt;&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;br style=&quot;box-sizing: border-box; margin: 0px; padding: 0px;&quot;/&gt;&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;二、解决：激活Windows server 2012&lt;br style=&quot;box-sizing: border-box; margin: 0px; padding: 0px;&quot;/&gt;&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;1、尝试使用激活工具激活（为什么使用激活工具了？原因大家懂得。。。&lt;img src=&quot;http://img.baidu.com/hi/jx2/j_0066.gif&quot; style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; border: none; vertical-align: middle;&quot;/&gt;没钱买正版。）&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp; &amp;nbsp;使用了几个工具，并没有激活成功，反而激活工具报错：评估版什么什么的。。。（记不清了。。）&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;2、发现右下角显示：&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp; &amp;nbsp;&lt;img src=&quot;/Upload/ueditor/image/20170109/1483942663258018.png&quot; title=&quot;1478504928361212.png&quot; alt=&quot;20161107143114.png&quot; style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; border: none; vertical-align: middle;&quot;/&gt;&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp; &amp;nbsp;这和我们所要求的系统版本不一致（我也不知道这什么版本。。。&lt;img src=&quot;http://img.baidu.com/hi/jx2/j_0066.gif&quot; style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; border: none; vertical-align: middle;&quot;/&gt;）&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp;3、遂百度之，无意瞥见：《&lt;a href=&quot;http://www.cnblogs.com/awpatp/archive/2013/03/21/2973646.html&quot; target=&quot;_blank&quot; title=&quot;如何将Windows Server 2012的Evaluation版本转为正式版?&quot; style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; color: rgb(51, 122, 183); text-decoration: none; background-color: transparent;&quot;&gt;如何将Windows Server 2012的Evaluation版本转为正式版?&lt;/a&gt;》&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp; &amp;nbsp;于是照步骤将系统转化为正式版：&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp; &amp;nbsp;①先查看系统版本及可转化的版本，CMD命令如下：&lt;/p&gt;&lt;pre class=&quot;brush:html;toolbar:false&quot; style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 9.5px; overflow: auto; font-family: Menlo, Monaco, Consolas, &amp;#39;Courier New&amp;#39;, monospace; font-size: 13px; line-height: 1.42857; color: rgb(51, 51, 51); word-break: break-all; word-wrap: break-word; border: 1px solid rgb(204, 204, 204); border-radius: 4px; background-color: rgb(245, 245, 245);&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;DISM&amp;nbsp;/online&amp;nbsp;/Get-TargetEditions&lt;/pre&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp; &amp;nbsp;&lt;img src=&quot;/Upload/ueditor/image/20170109/1483942664460353.png&quot; title=&quot;1478505219528966.png&quot; alt=&quot;20161107144430.png&quot; style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; border: none; vertical-align: middle;&quot;/&gt;&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp; &amp;nbsp;&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp; &amp;nbsp;②进行转化，输入CMD命令：&lt;/p&gt;&lt;pre class=&quot;brush:html;toolbar:false&quot; style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 9.5px; overflow: auto; font-family: Menlo, Monaco, Consolas, &amp;#39;Courier New&amp;#39;, monospace; font-size: 13px; line-height: 1.42857; color: rgb(51, 51, 51); word-break: break-all; word-wrap: break-word; border: 1px solid rgb(204, 204, 204); border-radius: 4px; background-color: rgb(245, 245, 245);&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;DISM&amp;nbsp;/online&amp;nbsp;/Set-Edition:ServerDatacenter&amp;nbsp;/ProductKey:XXXXXXXXXXXXXXXXXXX&amp;nbsp;/AcceptEula&lt;/pre&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&amp;nbsp; &amp;nbsp;&lt;span style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; color: rgb(102, 102, 102); font-family: &amp;#39;Helvetica Neue&amp;#39;, STHeiti, 微软雅黑, &amp;#39;Microsoft YaHei&amp;#39;, Helvetica, Arial, sans-serif;&quot;&gt;附安装密钥：&lt;/span&gt;&lt;br style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; color: rgb(102, 102, 102); font-family: &amp;#39;Helvetica Neue&amp;#39;, STHeiti, 微软雅黑, &amp;#39;Microsoft YaHei&amp;#39;, Helvetica, Arial, sans-serif;&quot;/&gt;&lt;span style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; color: rgb(102, 102, 102); font-family: &amp;#39;Helvetica Neue&amp;#39;, STHeiti, 微软雅黑, &amp;#39;Microsoft YaHei&amp;#39;, Helvetica, Arial, sans-serif;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;Windows Server 2012 Standard&lt;span style=&quot;box-sizing: border-box; margin: 0px; padding: 0px;&quot;&gt;密钥&lt;/span&gt;&amp;nbsp;- XC9B7-NBPP2-83J2H-RHMBY-92BT4&lt;/span&gt;&lt;br style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; color: rgb(102, 102, 102); font-family: &amp;#39;Helvetica Neue&amp;#39;, STHeiti, 微软雅黑, &amp;#39;Microsoft YaHei&amp;#39;, Helvetica, Arial, sans-serif;&quot;/&gt;&lt;span style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; color: rgb(102, 102, 102); font-family: &amp;#39;Helvetica Neue&amp;#39;, STHeiti, 微软雅黑, &amp;#39;Microsoft YaHei&amp;#39;, Helvetica, Arial, sans-serif;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;Windows Server 2012 Datacenter&lt;span style=&quot;box-sizing: border-box; margin: 0px; padding: 0px;&quot;&gt;密钥&lt;/span&gt;&amp;nbsp;- 48HP8-DN98B-MYWDG-T2DCC-8W83P&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;span style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; color: rgb(102, 102, 102); font-family: &amp;#39;Helvetica Neue&amp;#39;, STHeiti, 微软雅黑, &amp;#39;Microsoft YaHei&amp;#39;, Helvetica, Arial, sans-serif;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;img src=&quot;/Upload/ueditor/image/20170109/1483942665686787.png&quot; title=&quot;1478505369487798.png&quot; alt=&quot;20161107144644.png&quot; style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; border: none; vertical-align: middle;&quot;/&gt;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;span style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; color: rgb(102, 102, 102); font-family: &amp;#39;Helvetica Neue&amp;#39;, STHeiti, 微软雅黑, &amp;#39;Microsoft YaHei&amp;#39;, Helvetica, Arial, sans-serif;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 10px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;span style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; color: rgb(102, 102, 102); font-family: &amp;#39;Helvetica Neue&amp;#39;, STHeiti, 微软雅黑, &amp;#39;Microsoft YaHei&amp;#39;, Helvetica, Arial, sans-serif;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;span style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; font-family: arial, helvetica, sans-serif;&quot;&gt;③按&amp;quot;Y&amp;quot;键重启系统即可完成转化&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;box-sizing: border-box; margin-top: 0px; margin-bottom: 0px; padding: 0px; font-size: 14px; color: rgb(51, 51, 51); font-family: &amp;#39;Microsoft Yahei&amp;#39;, Helvetica, Arial, sans-serif; white-space: normal; background-color: rgb(255, 255, 255);&quot;&gt;&lt;span style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; color: rgb(102, 102, 102); font-family: arial, helvetica, sans-serif;&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; ④重启之后就可使用激活工具进行激活（激活工具自己找吧。。&lt;/span&gt;&lt;span style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; color: rgb(102, 102, 102); font-family: &amp;#39;Helvetica Neue&amp;#39;, STHeiti, 微软雅黑, &amp;#39;Microsoft YaHei&amp;#39;, Helvetica, Arial, sans-serif;&quot;&gt;。&lt;img src=&quot;http://img.baidu.com/hi/jx2/j_0059.gif&quot; style=&quot;box-sizing: border-box; margin: 0px; padding: 0px; border: none; vertical-align: middle;&quot;/&gt;）&lt;/span&gt;&lt;/p&gt;','15') [ RunTime:0.0040s ]
