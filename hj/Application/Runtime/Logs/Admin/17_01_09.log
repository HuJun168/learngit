&amp;nbsp;}
&amp;nbsp;&amp;nbsp;nginx_handled(){
&amp;nbsp;&amp;nbsp;/usr/bin/curl&amp;nbsp;&amp;quot;http://127.0.0.1/ngxstatus&amp;quot;&amp;nbsp;2&amp;gt;/dev/null|&amp;nbsp;awk&amp;nbsp;&amp;#39;NR==3{print&amp;nbsp;$2}&amp;#39;
&amp;nbsp;}
